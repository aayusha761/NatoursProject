{
  "name": "cssproject",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss sass/css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss sass/css/style.comp.css",
    "concat:css": "concat -o sass/css/style.concat.css sass/css/style.comp.css sass/css/icon-font.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' sass/css/style.concat.css -o sass/css/style.prefix.css",
    "compress:css": "node-sass sass/css/style.prefix.css sass/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Aayush",
  "license": "ISC",
  "description": "Natours Project",
  "devDependencies": {
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0"
  },
  "dependencies": {
    "autoprefixer": "^9.7.6",
    "concat": "^1.0.3",
    "jquery": "^3.5.0"
  }
}
